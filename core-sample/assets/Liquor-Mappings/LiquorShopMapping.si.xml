<database-mapping-descriptor>

	<entity table_name="LIQUOR_SHOP_MAPPING" class_name="siminov.core.sample.model.LiquorShopMapping">
		
		<relationships>

		    <many-to-many refer="liquor" refer_to="siminov.core.sample.model.Liquor" on_update="cascade" on_delete="cascade">
				<property name="load">true</property>
			</many-to-many>		
			
		    <many-to-many refer="shop" refer_to="siminov.core.sample.model.Shop" on_update="cascade" on_delete="cascade">
				<property name="load">true</property>
			</many-to-many>		
		    		    
		</relationships>
		
	</entity>

</database-mapping-descriptor>		
		