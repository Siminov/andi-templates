<entity-descriptor>

    <property name="table_name">LIQUOR_SHOP_MAPPING</property>
    <property name="class_name">siminov.core.sample.model.LiquorShopMapping</property>
    
	<relationships>

	    <relationship>
			<property name="type">many-to-many</property>
			<property name="refer">liquor</property>
			<property name="refer_to">siminov.core.sample.model.Liquor</property>
			<property name="on_update">cascade</property>
			<property name="on_delete">cascade</property>
			<property name="load">true</property>
		</relationship>		
		
	    <relationship>
	        <property name="type">many-to-many</property>
	        <property name="refer">shop</property>
	        <property name="refer_to">siminov.core.sample.model.Shop</property>
	        <property name="on_update">cascade</property>
	        <property name="on_delete">cascade</property>
			<property name="load">true</property>
		</relationship>		
	    		    
	</relationships>
		
</entity-descriptor>		
		