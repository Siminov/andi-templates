<siminov>

	<property name="name">SIMINOV SQLCIPHER TEMPLATE</property>	
	<property name="description">Siminov SqlCipher Template Application</property>
	<property name="version">0.9</property>

	<property name="load_initially">true</property>

	<!-- DATABASE-DESCRIPTORS -->
	<database-descriptors>
		<database-descriptor>DatabaseDescriptor.si.xml</database-descriptor>
	</database-descriptors>

	<!-- SIMINOV EVENTS -->
	<event-handlers>
	    <event-handler>siminov.orm.sqlcipher.template.events.SiminovEventHandler</event-handler>
	    <event-handler>siminov.orm.sqlcipher.template.events.DatabaseEventHandler</event-handler>
	</event-handlers>
			
</siminov>
